\newcommand{\part}[2][]{%
\stepcounter{part}%
\ifoptarg\cuthere{toc}{part}{#1}%
\else\cuthere{toc}{part}{#2}\fi
\@open{H1}{ALIGN=center}%
\partname:~\thepart\\%
#2%
\@close{H1}%
}
\newcommand{\part*}[1]{%
\@open{H1}{ALIGN=center}%
#1%
\@close{H1}%
}
\newcommand{\section}[2][]{%
\stepcounter{section}%
\ifoptarg\cuthere{toc}{section}{#1}%
\else\cuthere{toc}{section}{#2}\fi%
\@open{H2}{}%
\thesection\quad #2%
\@close{H2}%
}
\newcommand{\section*}[1]{%
\cuthere{toc}{section}{#1}%
\@open{H2}{}%
#1%
\@close{H2}%
}


\newcommand{\subsection}[2][]{%
\stepcounter{subsection}%
\ifoptarg\cuthere{toc}{subsection}{#1}%
\else\cuthere{toc}{subsection}{#2}\fi%
\@open{H3}{}%
\thesubsection\quad #2%
\@close{H3}%
}
\newcommand{\subsection*}[1]{%
\cuthere{toc}{subsection}{#1}%
\@open{H3}{}%
#1%
\@close{H3}%
}
\newcommand{\subsubsection}[2][]{%
\stepcounter{subsubsection}%
\ifoptarg\cuthere{toc}{subsubsection}{#1}%
\else\cuthere{toc}{subsubsection}{#2}\fi%
\@open{H4}{}%
\thesubsubsection\quad #2%
\@close{H4}%
}
\newcommand{\subsubsection*}[1]{%
\cuthere{toc}{subsubsection}{#1}%
\@open{H4}{}%
#1%
\@close{H4}%
}
\newcommand{\paragraph}[2][]{%
\@open{H5}{}%
#2%
\@close{H5}%
}
\newcommand{\paragraph*}[1]{%
\@open{H5}{}%
#1%
\@close{H5}%
}
\newcommand\abstractname{\iffrench Résumé~\else Abstract}
\newenvironment{abstract}{\begin{quote}{\bf \abstractname:}}{\end{quote}}
\newcommand{\@indexsection}[1]{\section*{#1}}
\newcommand{\@bibliosection}[1]{\section*{#1}}
\styleloadedtrue
\fi