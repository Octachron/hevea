\newcommand{\makeindex}{\newindex{default}{idx}{ind}{Index}}
\newcommand{\@seename}{\iffrench voir\else see\fi}
\newcommand{\see}[2]{\@seename\ \textit{#1}}
\newcommand{\indexspace}{\par}
\@endpackage{makeidx}